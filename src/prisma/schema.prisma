// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Installation {
  id                  String  @unique
  team                String
  user                String
  bot                 String
  enterprise          String
  tokenType           String
  isEnterpriseInstall Boolean
  appId               String
  authVersion         String
}
